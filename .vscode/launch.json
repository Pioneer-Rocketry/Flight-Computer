{
    "version": "0.2.0",
    "projectName": "Pioneer Rocketry Flight-Computer",
    "configurations": [
        {
            "name": "Debug with ST-Link",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/Code/build/Code.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "stutil",
            "device": "STM32F411RE",
            "interface": "swd",
            "runToEntryPoint": "main",
            "preLaunchTask": "Build and Upload"
        },
        {
            "name": "STlink attach",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/Code/build/Code.elf",
            "request": "attach",
            "type": "cortex-debug",
            "servertype": "stutil",
            "device": "STM32F411RE",
            "interface": "swd",
            "runToEntryPoint": "main",
            // "preLaunchTask": "Build",
        }
        
    ]
}